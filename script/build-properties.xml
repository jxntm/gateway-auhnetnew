<project name="properties">

  <property environment="env"/>

  <!-- Repository URLs -->
  <property name="repo-id" value="local-repo" />  
  <property name="read-repo-url" value="http://repo.campusops.net:8081/nexus/content/repositories/Release" />
  <property name="release-repo-url" value="http://repo.campusops.net:8081/nexus/content/repositories/jxntm-releases" />   	  
  <property name="snapshot-repo-url" value="http://repo.campusops.net:8081/nexus/content/repositories/jxntm-snapshots" />       

  <property name="build.version" value="1.0.0"/>
  <property name="build.source" value="1.5"/>
  <property name="build.debuglevel" value="lines,source"/>

  <property name="build.dir" value="${basedir}/build"/>
  <property name="build.classes.src.dir" value="${build.dir}/classes/src"/>
  <property name="build.doc.dir" value="${build.dir}/docs"/>
  <property name="build.lib.dir" value="${build.dir}/lib"/>

  <property name="src.dir" value="${basedir}/src"/>
</project>
